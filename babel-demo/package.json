{
  "name": "mini-demo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "babel src/main.js --out-file bundle.js",
    "commit": "cz",
    "lint": "eslint --config ./.eslintrc.js . --ext .js,.ts",
    "lint:fix": "eslint --config ./.eslintrc.js . --ext .js,.ts --fix",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "push": "cz && standard-version",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/runtime": "^7.18.9",
    "@vueuse/components": "^8.9.4",
    "@vueuse/core": "^8.9.4",
    "advjssdk": "^1.0.2",
    "billd-ui": "^1.2.5",
    "core-js": "^3.24.0",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.6"
  }
}
